{
  "name": "mete-boilerplate",
  "version": "1.0.0",
  "description": "react development env",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --coverage",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "dev": "cross-env NODE_ENV=development node server",
    "build:dll": "cross-env node ./scripts/dll.js"
  },
  "author": "KUICHU",
  "babel": {
    "plugins": [
      "styled-components"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-styled-components": "^1.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bundle-loader": "^0.5.5",
    "circular-dependency-plugin": "^4.3.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.4.2",
    "ip": "^1.1.5",
    "jest": "^22.0.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "less-vars-to-js": "^1.2.0",
    "ngrok": "^2.2.24",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.9",
    "postcss-nested": "^2.1.2",
    "postcss-plugin": "^1.0.0",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "antd": "^3.1.1",
    "axios": "^0.17.1",
    "es6-promise": "^4.1.1",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.4",
    "express": "^4.16.2",
    "fontfaceobserver": "^2.0.13",
    "history": "^4.7.2",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "offline-plugin": "^4.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sanitize.css": "^5.0.0",
    "styled-components": "^2.4.0"
  },
  "dllPlugin": {
    "path": "dll",
    "dlls": {
      "react_vendor": [
        "react",
        "react-dom",
        "react-router-dom"
      ]
    }
  }
}
